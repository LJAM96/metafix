# MetaFix Environment Variables
# Copy this file to .env and fill in your values

# Required: Secret key for encryption (generate with: openssl rand -hex 32)
SECRET_KEY=change-me-in-production

# Database URL (SQLite default, can be changed)
DATABASE_URL=sqlite+aiosqlite:///./data/metafix.db

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
